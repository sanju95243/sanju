class EmployeeManagementSys:
    def _init_(self, name, age, gender, salary, location):
        self.name = name
        self.age = age
        self.gender = gender
        self.salary = salary
        self.location = location

    def display(self):
        print("Name    :", self.name)
        print("Age     :", self.age)
        print("Gender  :", self.gender)
        print("Salary  :", self.salary)
        print("Location:", self.location)
        print("-" * 30)

# Creating employee objects
employee_1 = EmployeeManagementSys("Sanju", 19, "Female", 90000, "Barnala")
employee_2 = EmployeeManagementSys("Mandeep", 20, "Female", 70000, "Punjab")
employee_3 = EmployeeManagementSys("Sachin", 19, "Male", 80000, "Himachal Pradesh")
employee_4 = EmployeeManagementSys("Tania", 21, "Female", 90000, "Sangrur")
employee_5 = EmployeeManagementSys("Amandeep Singh", 23, "Male", 39000, "UP")

# Display employee details
print("Employee 1 Details:")
employee_1.display()

print("Employee 2 Details:")
employee_2.display()

print("Employee 3 Details:")
employee_3.display()

print("Employee 4 Details:")
employee_4.display()

print("Employee 5 Details:")
employee_5.display()
