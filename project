class Library_management_system:
    def _init_(self, title, author, price, pages):
        self.title=title
        self.author=author
        self.price=price
        self.pages=pages
    def title(self):
        return self.title
    def author(self):
        return self.author
    def price(self):
        return self.price
    def pages(self):
        return self.pages
book1=Library_management_system("wings of fire","william shakespeare",190, 300)
book2=Library_management_system("happy prince","oscar wild",250, 400)
book3=Library_management_system("story of life","william shakespeare",300, 170)
print("book1 title:",book1.title)
print("book2 tile:",book2.title)
print("book3 title:",book3.title)
print("book1 author:",book1.author)
print("book2 author:",book2.author)
print("book3 author:",book3.author)
print("book1 pages:",book1.pages)
print("book2 pages:",book2.pages)
print("book3 pages:",book3.pages)
print("book1 price:",book1.price)
print("book2 price:",book2.price)
print("book3 price:",book3.price)
